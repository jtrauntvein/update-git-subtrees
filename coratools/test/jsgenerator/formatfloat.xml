<config>
  <sources>
    <source name="ln" type="loggernet">
      <settings address="localhost" port="6789" logon="admin" password="admin"/>
    </source>
  </sources>
  <expressions>
    <expression>
      "ln:cr1000.public.settable"
    </expression>
    <expression>
      formatfloat("ln:cr1000.public.settable", $"%f")
    </expression>
    <expression>
      formatfloat("ln:cr1000.public.settable", $"%06.2f")
    </expression>
    <expression>
      formatfloatL("ln:cr1000.public.settable", $"%06.2e")
    </expression>
    <expression>
      formatFloat("ln:cr1000.public.settable", $"%-6.2g")
    </expression>
    <expression>
      formatFloat("ln:cr1000.public.settable", $"%+10.4g")
    </expression>
      
  </expressions>
</config>
